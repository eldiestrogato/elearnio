{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "data": {
      "type": "array",
      "items": [
        {
          "type": "object",
          "properties": {
            "id": { "type": "integer" },
            "courses": { "type": "array" },
            "name": { "type": "string" }
          },
          "required": ["id", "courses", "name"]
        }
      ]
    }
  },
  "required": ["data"]
}
