{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "data": {
      "type": "array",
      "items": [
        {
          "type": "object",
          "properties": {
            "id": { "type": "integer" },
            "talent_study_learning_paths": { "type": "array" },
            "talent_study_units": { "type": "array" },
            "name": { "type": "string" },
            "is_author": { "type": "boolean" }
          },
          "required": ["id", "talent_study_learning_paths", "talent_study_units", "is_author", "name"]
        }
      ]
    }
  },
  "required": ["data"]
}
